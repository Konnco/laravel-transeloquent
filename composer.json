{
  "name": "konnco/laravel-transeloquent",
  "description": "Laravel Eloquent Translatable",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "eloquent",
    "library",
    "translate"
  ],
  "authors": [
    {
      "name": "Franky So",
      "email": "frankyso.mail@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.9@dev",
    "mockery/mockery": "^1.2.3",
    "phpunit/phpunit": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "konnco\\Transeloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "konnco\\Transeloquent\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "konnco\\Transeloquent\\TranseloquentServiceProvider"
      ]
    }
  }
}
